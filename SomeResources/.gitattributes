#	## Masks for all files that should go to LFS
#
#	## iOS ##
#	*.a filter=lfs diff=lfs merge=lfs -text
#	*.assets filter=lfs diff=lfs merge=lfs -text
#	*.resS filter=lfs diff=lfs merge=lfs -text
#	*.dat filter=lfs diff=lfs merge=lfs -text
#	*.sks filter=lfs diff=lfs merge=lfs -text
#	*.assetbundle filter=lfs diff=lfs merge=lfs -text
#
#	*.unity3d filter=lfs diff=lfs merge=lfs -text
#	resources.resource filter=lfs diff=lfs merge=lfs -text
#	globalgamemanagers filter=lfs diff=lfs merge=lfs -text
#	unity_builtin_extra filter=lfs diff=lfs merge=lfs -text
#
#	## Unity generated ##
#	*il2cpp*.c filter=lfs diff=lfs merge=lfs -text
#	*il2cpp*.cpp filter=lfs diff=lfs merge=lfs -text
#
#	## Android ##
#	*.dll filter=lfs diff=lfs merge=lfs -text
#	*.resource filter=lfs diff=lfs merge=lfs -text
#	*.so filter=lfs diff=lfs merge=lfs -text
#
#	## Android ##
#	*.apk filter=lfs diff=lfs merge=lfs -text
#
#	## Unity ##
#	*__huge.asset filter=lfs diff=lfs merge=lfs -text
#	#*.cs diff=csharp text eol=crlf
#	#*.cginc text eol=crlf
#	#*.shader text eol=crlf
#	#*.mat merge=unityyamlmerge eol=crlf
#	#*.anim merge=unityyamlmerge eol=crlf
#	#*.unity merge=unityyamlmerge eol=crlf
#	#*.prefab merge=unityyamlmerge eol=crlf
#	#*.physicsMaterial2D merge=unityyamlmerge eol=crlf
#	#*.physicMaterial merge=unityyamlmerge eol=crlf
#	#*.asset merge=unityyamlmerge eol=crlf
#	#*.meta merge=unityyamlmerge eol=crlf
#	#*.controller merge=unityyamlmerge eol=crlf
#
#	## Images ##
#	*.ai filter=lfs diff=lfs merge=lfs -text
#	*.gif filter=lfs diff=lfs merge=lfs -text
#	*.hdr filter=lfs diff=lfs merge=lfs -text
#	*.HDR filter=lfs diff=lfs merge=lfs -text
#	*.jpeg filter=lfs diff=lfs merge=lfs -text
#	*.jpg filter=lfs diff=lfs merge=lfs -text
#	*.png filter=lfs diff=lfs merge=lfs -text
#	*.psd filter=lfs diff=lfs merge=lfs -text
#	*.tga filter=lfs diff=lfs merge=lfs -text
#	*.tif filter=lfs diff=lfs merge=lfs -text
#	*.tiff filter=lfs diff=lfs merge=lfs -text
#
#	## Audio ##
#	*.mp3 filter=lfs diff=lfs merge=lfs -text
#	*.ogg filter=lfs diff=lfs merge=lfs -text
#	*.wav filter=lfs diff=lfs merge=lfs -text
#
#	## Videos ##
#	*.mov filter=lfs diff=lfs merge=lfs -text
#	*.mp4 filter=lfs diff=lfs merge=lfs -text
#
#	## 3D Object ##
#	*.blend filter=lfs diff=lfs merge=lfs -text
#	*.FBX filter=lfs diff=lfs merge=lfs -text
#	*.fbx filter=lfs diff=lfs merge=lfs -text
#	*.obj filter=lfs diff=lfs merge=lfs -text
#
#	## etc. ##
#	*.aif filter=lfs diff=lfs merge=lfs -text
#	*.dll filter=lfs diff=lfs merge=lfs -text
#	*.exe filter=lfs diff=lfs merge=lfs -text
#	*.exr filter=lfs diff=lfs merge=lfs -text
#	*.lxo filter=lfs diff=lfs merge=lfs -text
#	*.pdf filter=lfs diff=lfs merge=lfs -text
#	*.pdb filter=lfs diff=lfs merge=lfs -text
#	*.reason filter=lfs diff=lfs merge=lfs -text
#	*.rns filter=lfs diff=lfs merge=lfs -text
#	*.tgz filter=lfs diff=lfs merge=lfs -text
#	*.ttf filter=lfs diff=lfs merge=lfs -text
#	*.unitypackage filter=lfs diff=lfs merge=lfs -text
#	*.zip filter=lfs diff=lfs merge=lfs -text
#	UnityFbxSdkNative filter=lfs diff=lfs merge=lfs -text
#
#	# Windows files should use crlf line endings
#	# https://help.github.com/articles/dealing-with-line-endings/
#	*.bat text eol=crlf
#
#	## WebGL build (it must be last rule) ##
#	/docs/** -filter=lfs